{
  "hosting": [
    {
      "target": "pro",
      "public": "dist",
      "cleanUrls": true,
      // next.jsのdefault behaviorに合わせる
      // https://nextjs.org/docs/api-reference/next.config.js/trailing-slash
      "trailingSlash": false,
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    },
    {
      "target": "dev",
      "public": "dist",
      "cleanUrls": true,
      // next.jsのdefault behaviorに合わせる
      // https://nextjs.org/docs/api-reference/next.config.js/trailing-slash
      "trailingSlash": false,
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    }
  ]
}

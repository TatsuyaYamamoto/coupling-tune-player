{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "ordered-imports": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": false,
    "no-var-requires": false,
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "object-literal-sort-keys": false,
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "no-empty-interface": false
  }
}
